// Here is the code for random fade ins and out, 1/15 chance by default. will last 15 and 1/3 beats if I did my math right

playernovisuals:
rest 0x62
random 0x19
switch
	case 0
		rest 1
		random 0xF
		switch
			case 2
			fade 1, 0, 0xBF
			break
			default
			fade 1, 0, 0x23F
			break
		endswitch
	break
	case 1
	break
	default
	break
